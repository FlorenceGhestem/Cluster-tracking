{
	"name": "Cluster-tracking",
	"build": {
			"dockerfile": "Dockerfile",
			"context": ".."
	},
	"extensions": [
			"ms-python.python",
			"ms-python.vscode-pylance",
			"ms-azuretools.vscode-docker"
	],
	"settings": {
			"python.pythonPath": "/usr/local/bin/python",
			"python.linting.enabled": true,
			"python.linting.pylintEnabled": true
	}
}
